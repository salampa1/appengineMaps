<queue-entries>
<!--Change the refresh rate of the default queue to 1/s-->
  <queue>
    <name>default</name>
    <rate>1/s</rate>
    
  </queue>
  
  <queue>
    <name>pushqueue</name>
    <rate>5/s</rate>
    <target>backend</target>
  </queue>
  
  <queue>
    <name>pullqueue</name>
    <mode>pull</mode>
  </queue>
</queue-entries>